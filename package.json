{
  "name": "sapatenis-ui",
  "version": "0.1.1",
  "private": true,
  "description": "React components",
  "license": "BSD-3-Clause-Clear",
  "author": "Matheus Rocha <matheus_rocha98@outlook.com> (https://mrocha.xyz/)",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrocha98/sapatenis-ui.git"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "pkg:run": "manypkg run",
    "pkg:fix": "manypkg fix",
    "pkg:check": "manypkg check",
    "dedup": "yarn-deduplicate --strategy highest yarn.lock",
    "postinstall": "yarn dedup && lerna exec sort-package-json --parallel && yarn pkg:check && preconstruct dev",
    "clean:full": "lerna clean --yes && rimraf node_modules",
    "clean:dist": "yarn lerna exec rimraf dist",
    "clean:storybook": "rimraf node_modules/.cache/storybook",
    "build": "yarn clean:dist && preconstruct build",
    "prestorybook:start": "yarn clean:storybook && preconstruct dev",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build && build-storybook && sb extract",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --maxWorkers=25% --watch",
    "test:ci": "jest --runInBand",
    "release": "yarn build && lerna version --force-publish"
  },
  "resolutions": {
    "**/react": "17.0.2",
    "**/react-dom": "17.0.2"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@babel/core": "~7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "~7.15.0",
    "@manypkg/cli": "~0.18.0",
    "@preconstruct/cli": "~2.1.5",
    "@storybook/addon-actions": "~6.3.8",
    "@storybook/addon-essentials": "~6.3.8",
    "@storybook/addon-links": "~6.3.8",
    "@storybook/addon-storysource": "~6.3.8",
    "@storybook/addons": "~6.3.8",
    "@storybook/node-logger": "~6.3.8",
    "@storybook/react": "~6.3.8",
    "@storybook/theming": "~6.3.8",
    "@styled-icons/fa-brands": "~10.38.0",
    "@styled-icons/fa-regular": "~10.34.0",
    "@styled-icons/fa-solid": "~10.34.0",
    "@testing-library/jest-dom": "~5.14.1",
    "@testing-library/react": "~12.0.0",
    "@testing-library/react-hooks": "~7.0.1",
    "@testing-library/user-event": "~13.2.1",
    "@types/jest": "~27.0.1",
    "@types/node": "~16.3.1",
    "@types/react": "~17.0.19",
