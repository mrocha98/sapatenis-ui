    "lint-staged": "~11.0.1",
    "nodemon": "~2.0.13",
    "npm-check-updates": "~11.8.2",
    "pkg-dir": "~4.2.0",
    "plop": "~2.7.4",
    "prettier": "2.3.2",
    "react": "~17.0.2",
